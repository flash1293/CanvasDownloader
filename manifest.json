{
  "manifest_version": 2,
  
  "name": "Canvas-Downloader",
  "description": "Makes downloading a canvas as an png possible",
  "version": "1.0",
  
  "permissions": [
    "activeTab"
  ],
  
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.11.2.min.js", "app.js"]
    }
  ]
}